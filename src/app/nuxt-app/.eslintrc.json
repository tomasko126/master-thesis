{
    "env": {
        "browser": true,
        "es2022": true,
        "vue/setup-compiler-macros": true
    },
    "extends": [
        "@nuxtjs/eslint-config-typescript"
    ],
    "parser": "vue-eslint-parser",
    "parserOptions": {
        "ecmaVersion": 13,
        "parser": "@typescript-eslint/parser"
    },
    "root": true,
    "rules": {
        "semi": "off",
        "comma-dangle": "off",
        "space-before-function-paren": "off"
    }
}
